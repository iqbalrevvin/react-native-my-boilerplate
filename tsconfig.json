{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["jest", "node"],
    "typeRoots": [
      "@types",
      "./node_modules/@types"
    ],
    "baseUrl": "..",
    "paths": {
      "@/*": ["./src/*"],
      // "types/*": ["./@types/*"]
    }
  },
  "target": "es2020",
  "jsx": "react",
  "module": "commonjs",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": false,
  "skipLibCheck": true  
}
